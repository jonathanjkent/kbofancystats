wBB <- mean(guts$wBB)
wHBP <- mean(guts$wHBP)
w1B <- mean(guts$w1B)
w2B <- mean(guts$w2B)
w3B <- mean(guts$w3B)
wHR <- mean(guts$wHR)
runSB <- mean(guts$runSB)
runCS <- mean(guts$runCS)
## Fix Basics
batters$X1B <- batters$H-batters$X2B-batters$X3B-batters$HR
pitchers$IP2 <- pitchers$IP
